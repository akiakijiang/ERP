	            type: 'POST',
	            url: 'ajax.php?act=update_logistic_service_note',
	            data: 'action_type=5&service_id='+service_id,
	            dataType: 'json',
	            error: function() {	
	            	alert('ajax请求错误,update_logistic_service_note service_id:' + service_id); 
	            },
	            success: function(data) {
	               result=true;
	           	}
	        }); 
	        if(result) {
	           alert('等待消费者寄回货物成功');
	           $('#cancel_wait').val('已等待消费者寄回货物');
	        }
		}
		
