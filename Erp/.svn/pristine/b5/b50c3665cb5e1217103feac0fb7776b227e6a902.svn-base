{foreach from=$sinri_plus item=plus_text key=plus_type}
<!-- 
	SINRI PLUS 20140401 
	圆通面单打印的时候打印上大头字（xx市，xx区）,和目前上海仓库中通快递大头字打印的一样，包括打印大头字内容和面单上的打印位置。
	http://localhost/erp/admin/print_shipping_order3.php?order_id=9&service_goods_back=0&carrier_id=20
-->
	{if $plus_type=="20140510_SHYT_01" }
		<div style="position:absolute;font-size:25pt;font-weight:bold;top:240px;left:560px;">{$plus_text}</div>
	{/if}
