<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<link href="styles/default.css" rel="stylesheet" type="text/css">
<link rel="stylesheet" href="js/style/zapatec/zpcal/themes/winter.css" />
<title>-V结果导出页面</title>
<!-- Calendar -->
<script type="text/javascript" src="js/style/zapatec/utils/zapatec.js"></script>
<script type="text/javascript"
	src="js/style/zapatec/zpcal/src/calendar.js"></script>
<script type="text/javascript"
	src="js/style/zapatec/zpcal/lang/calendar-en.js"></script>
<script type="text/javascript" src="misc/jquery.js"></script>
<script type="text/javascript">
	
</script>
<!-- Calendar /-->
</head>

<body>
	<fieldset>
		<legend>-V结果导出</legend>
		<ul class="sType">
			<form method="POST">
				开始时间： <input style="width: 90px" id="startCalendar"
					name="start_time" value="{$start_time}" /> <input type="button"
					value="..." id="startTrigger" />&nbsp;&nbsp;&nbsp;&nbsp;结束时间： <input
					style="width: 90px" id="endCalendar" name="end_time"
					value="{$end_time}" /> <input type="button" value="..."
					id="endTrigger" /> &nbsp;&nbsp;&nbsp;&nbsp;<input type="submit"
					name="export-v" value="导出-V结果CSV" />
			</form>
		</ul>
	</fieldset>

	{literal}
	<script type="text/javascript">
		Zapatec.Calendar.setup({
			weekNumbers : false,
			electric : false,
			inputField : "startCalendar",
			button : "startTrigger",
			ifFormat : "%Y-%m-%d",
			daFormat : "%Y-%m-%d"
		});
		Zapatec.Calendar.setup({
			weekNumbers : false,
			electric : false,
			inputField : "endCalendar",
			button : "endTrigger",
			ifFormat : "%Y-%m-%d",
			daFormat : "%Y-%m-%d"
		});
	</script>
	{/literal}
</body>
</html>
