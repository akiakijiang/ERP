<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<link href="../styles/default.css" rel="stylesheet" type="text/css" />
<!-- Loading Calendar JavaScript files -->
  <script type="text/javascript" src="../js/style/zapatec/utils/zapatec.js"></script>
  <script type="text/javascript" src="../js/style/zapatec/zpcal/src/calendar.js"></script>
  <script type="text/javascript" src="../js/style/zapatec/zpcal/lang/calendar-en.js"></script>
  <link rel="stylesheet" href="../js/style/zapatec/zpcal/themes/winter.css" />
</head>

<body>
<form>
从
<input type="text" size="20" id="begin" name="row[begin]" value="{$smarty.request.row.begin}" />
<input type="button" value="..." id="beginBtn" />
到
<input type="text" size="20" id="end" name="row[end]" value="{$smarty.request.row.end}" />
<input type="button" value="..." id="endBtn" />
<input type="hidden" name="act" value="query" />
<input type="submit" value="query" />
</form>
<table class="bWindow" style="width:700px;font-size:14px;">
<thead><tr><th>user type</th><th>orders</th><th>paied order</th></tr></thead>
<tbody>
{foreach from=$user_type_count item=user_type}
<tr>
<td>{$user_type.user_type}</td><td>{$user_type.order_count}</td><td>{$user_type.paied_order_count}</td>
</tr>
{/foreach}
</tbody>
</table>

{literal}
<script type="text/javascript">//<![CDATA[
    Zapatec.Calendar.setup({
      weekNumbers       : false,
      electric          : false,
      inputField        : "begin",
      button            : "beginBtn",
      ifFormat          : "%Y-%m-%d 00:00:00",
      daFormat          : "%Y-%m-%d 00:00:00"
    });
    
    Zapatec.Calendar.setup({
      weekNumbers       : false,
      electric          : false,
      inputField        : "end",
      button            : "endBtn",
      ifFormat          : "%Y-%m-%d 00:00:00",
      daFormat          : "%Y-%m-%d 00:00:00"
    });

    
//]]>
</script>
{/literal}
</body>
</html>